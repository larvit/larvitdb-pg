{
  "name": "larvitdb-pg",
  "version": "1.0.6",
  "description": "Database wrapper for pg (PostgreSQL)",
  "main": "build/index.js",
  "files": [
    "/build"
  ],
  "scripts": {
    "build": "npm run lint && tsc && cp src/models.d.ts build/",
    "lint": "tslint ./src/* ./test/*",
    "test": "npm run lint && npm run build && nyc --reporter=lcov --require ts-node/register tape test/**/*.{ts,js} | tap-spec; nyc report ---reporter=text"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/larvit/larvitdb-pg.git"
  },
  "keywords": [
    "db",
    "database",
    "wrapper",
    "postgres",
    "postgresql"
  ],
  "author": {
    "name": "Mikael 'Lilleman' GÃ¶ransson",
    "email": "lilleman@zmartbag.com"
  },
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/larvit/larvitdb-pg/issues"
  },
  "homepage": "https://github.com/larvit/larvitdb-pg#readme",
  "dependencies": {
    "larvitutils": "3.2.1",
    "pg": "8.13.1"
  },
  "devDependencies": {
    "@types/dotenv": "8.2.0",
    "@types/node": "14.14.7",
    "@types/pg": "8.11.10",
    "@types/tape": "4.13.0",
    "dotenv": "8.2.0",
    "coveralls": "3.1.0",
    "nyc": "15.1.0",
    "tap-spec": "5.0.0",
    "tape": "5.0.1",
    "ts-node": "9.0.0",
    "tslint": "6.1.3",
    "typescript": "4.0.5"
  },
  "nyc": {
    "include": [
      "src/**/*.ts"
    ],
    "extension": [
      ".ts"
    ]
  }
}
