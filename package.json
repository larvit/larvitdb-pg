  "repository": {
    "type": "git",
    "url": "git+https://github.com/larvit/larvitdb-pg.git"
  },
  "keywords": [
    "db",
    "database",
    "wrapper",
    "postgres",
    "postgresql"
  ],
  "author": {
    "name": "Mikael 'Lilleman' GÃ¶ransson",
    "email": "lilleman@zmartbag.com"
  },
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/larvit/larvitdb-pg/issues"
  },
  "homepage": "https://github.com/larvit/larvitdb-pg#readme",
  "dependencies": {
    "larvitutils": "3.2.1",
    "pg": "7.14.0"
  },
  "devDependencies": {
    "@types/dotenv": "6.1.1",
    "@types/node": "12.12.14",
    "@types/pg": "7.11.2",
    "@types/tape": "4.2.33",
    "dotenv": "8.2.0",
    "coveralls": "3.0.9",
    "nyc": "14.1.1",
    "tap-spec": "5.0.0",
    "tape": "4.11.0",
    "ts-node": "8.5.4",
    "tslint": "5.20.1",
    "typescript": "3.7.3"
  },
  "nyc": {
    "include": [
      "src/**/*.ts"
