{
  "name": "larvitdb-pg",
  "version": "1.0.1",
  "description": "Database wrapper for pg (PostgreSQL)",
  "main": "build/index.js",
  "files": [
    "/build"
  ],
  "scripts": {
    "build": "npm run lint && tsc",
    "lint": "tslint ./src/* ./test/*",
